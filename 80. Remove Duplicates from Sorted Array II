class Solution {
public:
    int removeDuplicates(vector<int>& nums) 
    {
        //vector <int> num;
if ( nums.empty())
{
     return 0;
}
        int i=1;
        int cnt = 1;
        int k=1;
     for( ;i<nums.size();i++)
         {
              if (nums[i]== nums[i-1])
            {
                cnt++;
            }
         else{
            cnt= 1;
         }
         if ( cnt <=2)
         {
            nums[k]= nums[ i];
            k++;
         }
         }
         return k;
    }

};
