class Solution {
public:
    int majorityElement(vector<int>& nums) {
        vector<int> B;
        unique_copy(nums.begin(), nums.end(), back_inserter(B));
        
       
        int n = nums.size();
        for(int i = 0; i < B.size(); i++) 
        {
            int cnt = 0;
            for(int j = 0; j < n; j++) 
            {
                if(nums[j] == B[i]) 
                {
                    cnt++;
                }
            }
            if(cnt > n/2) {
                return B[i];
            }
        }
        
        return -1; 
    }
};
